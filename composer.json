{
	"name": "pieldefoca/lux",
	"description": "Laravel admin panel",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "David Aloa",
			"email": "betibaskonia@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Pieldefoca\\Lux\\": "src",
			"Pieldefoca\\Lux\\Tests\\": "tests"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"Pieldefoca\\Lux\\LuxServiceProvider"
			]
		}
	},
	"require-dev": {
		"orchestra/testbench": "^8.0",
		"pestphp/pest": "^2.34"
	},
	"require": {
		"livewire/livewire": "^3.0",
		"spatie/laravel-translatable": "^6.5",
		"laravel/prompts": "^0.1.13",
		"spatie/laravel-permission": "^6.1",
		"spatie/image": "^3.4",
		"spatie/laravel-sitemap": "^7.2"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
